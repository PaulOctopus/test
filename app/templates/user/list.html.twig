<html>
    <head>
        <title>User list</title>
    </head>
    <body>
        <table>
            <thead>
            <tr>
                <th>Id</th>
                <th>Login</th>
            </tr>
            </thead>
            {% for user in users %}
                <tr>
                    <td>{{ user.id }}</td>
                    <td>{{ user.login }}</td>
                </tr>
            {% endfor %}
        </table>
    </body>
</html>

